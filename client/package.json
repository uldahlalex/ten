  "dependencies": {
    "@tailwindcss/vite": "^4.*",
    "daisyui": "^5.*",
    "jotai": "^2.*",
    "jotai-devtools": "^0.*",
    "jwt-decode": "^4.0.0",
    "react": "^19.*",
    "react-dom": "^19.*",
    "react-error-boundary": "^6.0.0",
    "react-hot-toast": "^2.*",
    "react-router": "^7.*",
    "react-router-dom": "^7.*",
    "tailwindcss": "^4.*"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@types/react": "^19.*",
    "@types/react-dom": "^19.*",
    "@typescript-eslint/eslint-plugin": "^8.*",
    "@typescript-eslint/parser": "^8.*",
    "@vitejs/plugin-react": "^4.*",
    "dependency-cruiser": "^16.*",
    "eslint": "^9.*",
    "eslint-plugin-import": "^2.*",
    "eslint-plugin-react-hooks": "5.*",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "typescript": "^5.*",
    "typescript-eslint": "^8.35.1",
    "vite": "^6.*"
  }
